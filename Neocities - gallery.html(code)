  .nav-collapse .nav > li > a:hover, .nav-collapse .nav > li > a:focus, .na=
v-collapse .dropdown-menu a:hover, .nav-collapse .dropdown-menu a:focus { b=
ackground-color: rgb(242, 242, 242); }
